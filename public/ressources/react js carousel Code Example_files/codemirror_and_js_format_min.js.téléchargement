var codes = document.getElementsByClassName("code_mirror_code");

for(var i =0;i<codes.length;i++){
    var answerCodeMirror = TaysCodeMirror.fromTextArea(codes[i],{
                lineNumbers: true,
                theme:"prism-okaidia",
                mode: 'javascript',
                viewportMargin: Infinity,
    });
}

var codes2 = document.getElementsByClassName("TaysCodeMirror");
for(let i =0;i<codes2.length;i++){
                codes2[i].addEventListener("copy", function(){

                var geo_answer_result_id=this.previousSibling.getAttribute("geo_answer_result_id");

                    makeRequest('GET', "/api/page_log.php?action=copied&geo_answer_result_id="+geo_answer_result_id);

                     gtag('event', 'conversion', {
                      'send_to': 'AW-1030955762/w3xgCL7-ht8BEPLFzOsD',
                      'value': 1.0,
                      'currency': 'USD',
                      'aw_remarketing_only': true
                  });
                });


              let enterX,leaveX,enterY,leaveY,readTimeTotal,readTimeStart,readTimeEnd;
              let tClicks=0;
              codes2[i].addEventListener("click", function(e){
                    tClicks+=1;
              }.bind(this));
              codes2[i].addEventListener("mouseenter", function(e){
                    enterX=e.offsetX;
                    enterY=e.offsetY;
                    readTimeStart = new Date().getTime();
              }.bind(this));

              codes2[i].addEventListener("mouseleave", function(e){
                var geo_answer_result_id=this.previousSibling.getAttribute("geo_answer_result_id");

                    leaveX=e.offsetX;
                    leaveY=e.offsetY;
                    readTimeEnd = new Date().getTime();
                   
                     //get direction
                     //this means down, does not count
                     if(Math.atan2((leaveY-enterY), (leaveX-enterX)) * 180 / Math.PI > 45 && Math.atan2((leaveY-enterY), (leaveX-enterX)) * 180 / Math.PI < 135){
                        return;
                     }

                    if((readTimeEnd - readTimeStart) > 500 ){
                       makeRequest('GET', "/api/page_log.php?action=answer_read&geo_answer_result_id="+geo_answer_result_id+"&ht="+(readTimeEnd-readTimeStart));
                    }

              });

}


